{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-sonarts"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "prefer-for-of": false,
        "max-classes-per-file": false,
        "no-angle-bracket-type-assertion": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}
